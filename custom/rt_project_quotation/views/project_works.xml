<odoo>
    <data>

        <!--      PROJECT WORKS-->
        <record model="ir.ui.view" id="planned_work_tree_view">
            <field name="name">planned.work.tree.view</field>
            <field name="model">planned.work</field>
            <field name="arch" type="xml">
                <tree string="Planned Work">
                    <field name="name" string="Project Section"/>
                    <field name="project_category" string="Project"/>
                    <field name="quote_id" string="Quote"/>
                </tree>
            </field>
        </record>

        <record model="ir.ui.view" id="planned_work_form_view">
            <field name="name">planned.work.form.view</field>
            <field name="model">planned.work</field>
            <field name="arch" type="xml">
                <form>
                    <!--                    <header></header>-->
                    <sheet string="Project Work">
                        <div class="oe_title">
                            <group>
                                <field name="project_category" string="Project Category"/>
                            </group>
                            <h1 class="o_row">
                                <field name="name" placeholder="Project Work..." domain="[('project_category','=',project_category)]"/>
                            </h1>
                            <group>
                                <field name="labour_rate" string="Labour Rate(%)" placeholder="Input the % of labour chargable per section..."/>
                            </group>
                        </div>
                        <group>
                            <field name="materials_used"/>
                        </group>
                        <group>
                            <field name="quote_id" string="Quote"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

